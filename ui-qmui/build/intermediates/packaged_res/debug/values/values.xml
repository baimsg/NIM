<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="reference" name="QMUIPullLayoutStyle"/>
    <declare-styleable name="QMUIPullLayout">
        <attr name="qmui_pull_enable_edge">
            <flag name="left" value="0x1"/>
            <flag name="top" value="0x2"/>
            <flag name="right" value="0x4"/>
            <flag name="bottom" value="0x8"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="QMUIPullLayout_Layout">
        <attr format="boolean" name="qmui_is_target"/>
        <attr format="enum" name="qmui_pull_edge">
            <enum name="left" value="0x1"/>
            <enum name="top" value="0x2"/>
            <enum name="right" value="0x4"/>
            <enum name="bottom" value="0x8"/>
        </attr>
        <attr format="dimension" name="qmui_action_view_init_offset"/>
        <attr format="dimension|enum" name="qmui_target_view_trigger_offset">
            <enum name="wrap" value="-2"/>
        </attr>
        <attr format="boolean" name="qmui_can_over_pull"/>
        <attr format="float" name="qmui_pull_rate"/>
        <attr format="float" name="qmui_received_fling_fraction"/>
        <attr format="boolean" name="qmui_need_receive_fling_from_target_view"/>
        <attr format="integer" name="qmui_scroll_speed_per_pixel"/>
        <attr format="boolean" name="qmui_trigger_until_scroll_to_trigger_offset"/>
        <attr format="boolean" name="qmui_scroll_to_trigger_offset_after_touch_up"/>
    </declare-styleable>
</resources>